[alias]
  cleanup = !git checkout master && git pull && git branch  --merged  master | grep -v 'master$' | xargs  git  branch  -d
  unstage = reset HEAD
  st = status

[apply]
  whitespace = nowarn
[core]
    pager = /usr/local/opt/git/share/git-core/contrib/diff-highlight/diff-highlight | less
    editor = /usr/local/bin/nvim
[push]
  default = current
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color]
  ui = auto
[diff]
  wordRegex = [^[:space:]]
  algorithm = patience
[help]
	autocorrect = 1
[advice]
	statusHints = false
[branch]
	autosetuprebase = always
[fetch]
	prune = true
[pull]
	rebase = true
[rebase]
	autostash = true
[log]
  decorate = true
[format]
  pretty = oneline
[rerere]
	enabled = true
[http "https://gopkg.in"]
    followRedirects = true
